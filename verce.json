{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "_roxi/.roxi/*.js",
        "use": "@vercel/node",
        "config": { "includeFiles": "build/**" }
      }
    ]
  }